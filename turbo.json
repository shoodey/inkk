{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "format:self": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "//#format:self": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  }
}
