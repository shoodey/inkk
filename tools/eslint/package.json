{
  "name": "@inkk/eslint-config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./base": "./base.js",
    "./react": "./react.js"
  },
  "scripts": {
    "format:self": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "typecheck": "tsc",
    "clean": "git clean -xdf .cache .turbo node_modules"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-turbo": "^2.5.2",
    "typescript-eslint": "^8.32.0"
  },
  "devDependencies": {
    "@inkk/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:"
  }
}
